<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".views.Register_User">

    <EditText
        android:id="@+id/register_usr_password_confirm5"
        android:layout_width="205dp"
        android:layout_height="48dp"
        android:ems="10"
        android:inputType="textPassword"
        app:layout_constraintEnd_toEndOf="@+id/register_usr_password_confirm4"
        app:layout_constraintStart_toStartOf="@+id/register_usr_password_confirm4"
        app:layout_constraintTop_toBottomOf="@+id/textView11" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:text="@string/register_usr_password_confirm"
        app:layout_constraintEnd_toStartOf="@+id/register_usr_password_confirm5"
        app:layout_constraintStart_toStartOf="@+id/register_usr_password_confirm5"
        app:layout_constraintTop_toBottomOf="@+id/register_usr_password_confirm4" />

    <EditText
        android:id="@+id/register_usr_password_confirm4"
        android:layout_width="205dp"
        android:layout_height="48dp"
        android:ems="10"
        android:inputType="textPassword"
        app:layout_constraintEnd_toEndOf="@+id/register_usr_password_confirm2"
        app:layout_constraintStart_toStartOf="@+id/register_usr_password_confirm2"
        app:layout_constraintTop_toBottomOf="@+id/textView10" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:text="@string/register_usr_password_confirm"
        app:layout_constraintEnd_toStartOf="@+id/register_usr_password_confirm4"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/register_usr_password_confirm4"
        app:layout_constraintTop_toBottomOf="@+id/register_usr_password_confirm2" />

    <EditText
        android:id="@+id/register_usr_password_confirm2"
        android:layout_width="205dp"
        android:layout_height="48dp"
        android:ems="10"
        android:inputType="textPassword"
        app:layout_constraintEnd_toEndOf="@+id/register_usr_password_confirm"
        app:layout_constraintStart_toStartOf="@+id/register_usr_password_confirm"
        app:layout_constraintTop_toBottomOf="@+id/textView9" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:text="@string/register_usr_password"
        app:layout_constraintEnd_toStartOf="@+id/register_usr_password_confirm2"
        app:layout_constraintStart_toStartOf="@+id/register_usr_password_confirm2"
        app:layout_constraintTop_toBottomOf="@+id/register_usr_password_confirm" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:text="@string/register_usr_bdate"
        app:layout_constraintEnd_toStartOf="@+id/register_usr_password_confirm"
        app:layout_constraintStart_toStartOf="@+id/register_usr_password_confirm"
        app:layout_constraintTop_toBottomOf="@+id/register_usr_password" />

    <EditText
        android:id="@+id/register_usr_password_confirm"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPassword"
        app:layout_constraintEnd_toEndOf="@+id/register_usr_password"
        app:layout_constraintStart_toStartOf="@+id/register_usr_password"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:text="@string/register_usr_title"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/register_usr_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textEmailAddress"
        app:layout_constraintEnd_toEndOf="@+id/textView2"
        app:layout_constraintStart_toStartOf="@+id/textView2"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <EditText
        android:id="@+id/register_usr_email"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textEmailAddress"
        app:layout_constraintEnd_toEndOf="@+id/register_usr_name"
        app:layout_constraintStart_toStartOf="@+id/register_usr_name"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <EditText
        android:id="@+id/register_usr_password"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPassword"
        app:layout_constraintEnd_toEndOf="@+id/register_usr_email"
        app:layout_constraintStart_toStartOf="@+id/register_usr_email"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="50dp"
        android:text="@string/register_usr_username"
        app:layout_constraintEnd_toStartOf="@+id/register_usr_name"
        app:layout_constraintStart_toStartOf="@+id/register_usr_name"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:text="@string/register_usr_email"
        app:layout_constraintEnd_toStartOf="@+id/register_usr_email"
        app:layout_constraintStart_toStartOf="@+id/register_usr_email"
        app:layout_constraintTop_toBottomOf="@+id/register_usr_name" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:text="@string/register_usr_phone"
        app:layout_constraintEnd_toStartOf="@+id/register_usr_password"
        app:layout_constraintStart_toStartOf="@+id/register_usr_password"
        app:layout_constraintTop_toBottomOf="@+id/register_usr_email" />
</androidx.constraintlayout.widget.ConstraintLayout>